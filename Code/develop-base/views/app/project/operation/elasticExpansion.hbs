function doEdit(id,status) {
    var newStatus = 0;
    if(status==1){
        newStatus=0;
    }
    else{
        newStatus=1;
    }
    $.ajax({
        url: '{{projcfg.appurl}}/api/project/operation/elasticExpantion',
        type: 'post',
        // data: $('#colonyForm').serializeJson(),
        data:{id:id,status:newStatus},
        success: function (data) {
            $.messager.progress('close');
            if(data.success) {
                msgSuccess(data.msg);
                $("#panelBody").clean();
                initData();
            }
            else {
                msgError(data.msg+',错误代码:'+data.code);
            }
        }
    });
}

