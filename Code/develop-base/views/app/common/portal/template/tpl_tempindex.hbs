<script src="{{projcfg.appurl}}/static/js/echart/v3.1.3/echarts.min.js"></script>
<link type="text/css" rel="stylesheet" href="{{projcfg.appurl}}/static/css/page-style.css">
<style type="text/css">
    body{
        margin:0;
        padding:0;
    }

    .devops-bg{
        width:100%;
        height: 100%;
        overflow: hidden;
    }
    .devops-bg img{
        width:100%;
    }
    .devops-link a{
        display: block;
        width:50%;
        height:50%;
        z-index: 1;
        position: fixed;
    }
    .devops-link a:nth-child(1){
        left:0;
        top:0;
    }
    .devops-link a:nth-child(2){
        left:50%;
        top:0;
    }
    .devops-link a:nth-child(3){
        left:0;
        bottom:0;
    }
    .devops-link a:nth-child(4){
        left:50%;
        bottom:0;
    }
</style>
<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-body">
                <div class="devops-bg">
                    <img src="static/images/devops-img.png">
                </div>
                <div class="devops-link">
                    <a href="{{projcfg.appurl}}/colonym"></a>
                    <a href="{{projcfg.appurl}}/projectm"></a>
                    <a href="{{projcfg.appurl}}/operation_center"></a>
                    <a href="{{projcfg.appurl}}/imagewarehouse"></a>
                </div>
            </div>
        </div>
    </div>
</div>
